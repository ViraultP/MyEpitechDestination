C,MAAA,MAAM,EAAE,GAAE,CACP,MADK,GAEL,QAFK,CAEI,2BAFJ,CAJwB;AAOhC,MAAA,KAAK,EAAE,GAAE,CACN,MADI,GAEJ,QAFI,CAEK,0BAFL,EAGJ,KAHI,CAGE,iBAHF,EAIJ,GAJI,CAIA,EAJA,EAII,8CAJJ,CAPyB;AAYhC,MAAA,YAAY,EAAE,GAAE,CACb,MADW,GAEX,QAFW,CAEF,iCAFE,EAGX,GAHW,CAGP,CAHO,EAGJ,6CAHI,EAIX,GAJW,CAIP,EAJO,EAIH,8CAJG;AAZkB,KAAnB,CAAf;AAmBA,WAAO;AACL,MAAA,UAAU,EAAE,KADP;AAEL,MAAA,OAAO,EAAE,KAFJ;AAGL,MAAA,OAAO,EAAE,EAHJ;AAIL,MAAA,MAAM,EAAN;AAJK,KAAP;AAMD,GAjCY;AAkCb,EAAA,QAAQ,EAAE;AACR,IAAA,QADQ,sBACG;AACT,aAAO,KAAK,MAAL,CAAY,KAAZ,CAAkB,IAAlB,CAAuB,MAAvB,CAA8B,QAArC;AACD;AAHO,GAlCG;AAuCb,EAAA,OAvCa,qBAuCH;AACR,QAAI,KAAK,QAAT,EAAmB;AACjB,WAAK,OAAL,CAAa,IAAb,CAAkB,UAAlB;AACF;AACD,GA3CY;AA4Cb,EAAA,OAAO,EAAE;AACP,IAAA,cADO,0BACQ,IADR,EACc;AAAA;;AACnB,WAAK,OAAL,GAAe,EAAf;AACA,WAAK,UAAL,GAAkB,KAAlB;AACA,WAAK,OAAL,GAAe,IAAf;AAEA,WAAK,MAAL,CAAY,QAAZ,CAAqB,eAArB,EAAsC,IAAtC,EAA4C,IAA5C,CACE,UAAC,IAAD,EAAU;AACR,QAAA,KAAI,CAAC,OAAL,GAAe,IAAI,CAAC,OAApB;AACA,QAAA,KAAI,CAAC,UAAL,GAAkB,IAAlB;AACA,QAAA,KAAI,CAAC,OAAL,GAAe,KAAf;AACD,OALH,EAME,UAAC,KAAD,EAAW;AACT,QAAA,KAAI,CAAC,OAAL,GACG,KAAK,CAAC,QAAN,IACC,KAAK,CAAC,QAAN,CAAe,IADhB,IAEC,KAAK,CAAC,QAAN,CAAe,IAAf,CAAoB,OAFtB,IAGA,KAAK,CAAC,OAHN,IAIA,KAAK,CAAC,QAAN,EALF;AAMA,QAAA,KAAI,CAAC,UAAL,GAAkB,KAAlB;AACA,QAAA,KAAI,CAAC,OAAL,GAAe,KAAf;AACF,OAfF;AAiBD;AAvBM;AA5CI,CAAf","sourcesContent":["<template>\r\n  <div class=\"col-md-12\">\r\n    <div class=\"card card-container\">\r\n      <Form @submit=\"handleRegister\" :validation-schema=\"schema\">\r\n        <div v-if=\"!successful\">\r\n\r\n          <div class=\"form-group\">\r\n            <h3>Nom</h3>\r\n            <Field name=\"nom\" type=\"text\" class=\"form-control\" />\r\n            <ErrorMessage name=\"nom\" class=\"error-feedback\" />\r\n          </div>\r\n\r\n          <div class=\"form-group\">\r\n            <label for=\"prénom\">Prénom</label>\r\n            <Field name=\"prénom\" type=\"text\" class=\"form-control\" />\r\n            <ErrorMessage name=\"prénom\" class=\"error-feedback\" />\r\n          </div>\r\n\r\n          <div class=\"form-group\">\r\n            <label for=\"email\">Email</label>\r\n            <Field name=\"email\" type=\"email\" class=\"form-control\" />\r\n            <ErrorMessage name=\"email\" class=\"error-feedback\" />\r\n          </div>\r\n\r\n          <div class=\"form-group\">\r\n            <label for=\"mot_de_passe\">Mot de passe</label>\r\n            <Field name=\"mot_de_passe\" type=\"password\" class=\"form-control\" />\r\n            <ErrorMessage name=\"mot_de_passe\" class=\"error-feedback\" />\r\n          </div>\r\n\r\n          <div class=\"form-group\">\r\n            <button class=\"btn btn-primary btn-block\" :disabled=\"loading\">\r\n              <span\r\n                v-show=\"loading\"\r\n                class=\"spinner-border spinner-border-sm\"\r\n              ></span>\r\n              S'enregistrer\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </Form>\r\n\r\n      <div\r\n        v-if=\"message\"\r\n        class=\"alert\"\r\n        :class=\"successful ? 'alert-success' : 'alert-danger'\"\r\n      >\r\n        {{ message }}\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { Form, Field, ErrorMessage } from \"vee-validate\";\r\nimport * as yup from \"yup\";\r\n\r\nexport default {\r\n  name: \"Register\",\r\n  components: {\r\n    Form,\r\n    Field,\r\n    ErrorMessage,\r\n  },\r\n  data() {\r\n    const schema = yup.object().shape({\r\n      nom: yup\r\n        .string()\r\n        .required(\"Un nom est nécessaire!\"),\r\n      prénom: yup\r\n        .string()\r\n        .required(\"Un prénom est nécessaire!\"),\r\n      email: yup\r\n        .string()\r\n        .required(\"Un email est nécessaire!\")\r\n        .email(\"Email invalide!\")\r\n        .max(50, \"L'email doit contenir maximum 50 caractères!\"),\r\n      mot_de_passe: yup\r\n        .string()\r\n        .required(\"Un mot de passe est nécessaire!\")\r\n        .min(6, \"Il doit y avoir un minimum de 6 caractères!\")\r\n        .max(40, \"Il doit y avoir un maximum de 40 caractères!\"),\r\n    });\r\n\r\n    return {\r\n      successful: false,\r\n      loading: false,\r\n      message: \"\",\r\n      schema,\r\n    };\r\n  },\r\n  computed: {\r\n    loggedIn() {\r\n      return this.$store.state.auth.status.loggedIn;\r\n    },\r\n  },\r\n  mounted() {\r\n    if (this.loggedIn) {\r\n      this.$router.push(\"/profile\");\r\n    }\r\n  },\r\n  methods: {\r\n    handleRegister(user) {\r\n      this.message = \"\";\r\n      this.successful = false;\r\n      this.loading = true;\r\n\r\n      this.$store.dispatch(\"auth/register\", user).then(\r\n        (data) => {\r\n          this.message = data.message;\r\n          this.successful = true;\r\n          this.loading = false;\r\n        },\r\n        (error) => {\r\n          this.message =\r\n            (error.response &&\r\n              error.response.data &&\r\n              error.response.data.message) ||\r\n            error.message ||\r\n            error.toString();\r\n          this.successful = false;\r\n          this.loading = false;\r\n        }\r\n      );\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":""}]}